{
	"yadamu": {
		"FILE": "yadamu.json"
	},
	"yadamuDBI": {
		"MODE": "DATA_ONLY",
		"ON_ERROR": "SKIP",
		"SPATIAL_FORMAT": "WKB",
		"TABLE_MAX_ERRORS": 10,
		"TOTAL_MAX_ERRORS": 100,
		"BATCH_SIZE": 10000,
		"COMMIT_RATIO": 1
	},
	"drivers": {
		"file": "../dbi/file/fileDBI.js",
		"loader": "../dbi/loader/loaderDBI.js",
		"awsS3": "../dbi/awsS3/awsS3DBI.js",
		"azure": "../dbi/azure/azureDBI.js",
		"oracle": "../dbi/oracle/oracleDBI.js",
		"postgres": "../dbi/postgres/postgresDBI.js",
		"vertica": "../dbi/vertica/verticaDBI.js",
		"redshift": "../dbi/redshift/redshiftDBI.js",
		"mssql": "../dbi/mssql/mssqlDBI.js",
		"mysql": "../dbi/mysql/mysqlDBI.js",
		"mariadb": "../dbi/mariadb/mariadbDBI.js",
		"mongodb": "../dbi/mongodb/mongoDBI.js",
		"snowflake": "../dbi/snowflake/snowflakeDBI.js",
		"teradata": "../dbi/teradata[worker]teradataDBI.js"
	},
	"ddlEnabled": [
		"oracle"
	],
	"mariadb": {
		"TREAT_TINYINT1_AS_BOOLEAN": true
	},
	"mongodb": {
		"PORT": 27017,
		"MONGO_SAMPLE_LIMIT": 1000,
		"MONGO_STORAGE_FORMAT": "DOCUMENT",
		"MONGO_EXPORT_FORMAT": "ARRAY",
		"MONGO_STRIP_ID": false,
		"SPATIAL_FORMAT": "GeoJSON",
		"DEFAULT_STRING_LENGTH": "32",
		"MAX_STRING_LENGTH": "16777216"
	},
	"mssql": {
		"YADAMU_USER": "dbo",
		"SPATIAL_MAKE_VALID": false
	},
	"mysql": {
		"READ_KEEP_ALIVE": 0,
		"TREAT_TINYINT1_AS_BOOLEAN": true
	},
	"oracle": {
		"BATCH_LOB_COUNT": 1024,
		"LOB_MIN_SIZE": 32768,
		"LOB_MAX_SIZE": 16777216,
		"LOB_CACHE_COUNT": 50000,
		"ORACLE_XML_TYPE": "XML",
		"ORACLE_JSON_TYPE": "JSON",
		"OBJECT_FORMAT": "NATIVE",
		"MIGRATE_JSON_STORAGE": false,
		"TREAT_RAW1_AS_BOOLEAN": true,
		"SPATIAL_FORMAT": "WKB"
	},
	"postgres": {
		"SPATIAL_FORMAT": "WKB"
	},
	"snowflake": {
		"YADAMU_USER": "public",
		"MAX_CHARACTER_SIZE": 16777216,
		"MAX_BINARY_SIZE": 8388608,
		"TRANSIENT_TABLES": true,
		"DATA_RETENTION_TIME": 0,
		"SPATIAL_FORMAT": "WKB",
		"ORACLE_XML_TYPE": "VARIANT"
	}
}